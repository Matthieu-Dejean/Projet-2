{% extends 'layout.html.twig' %}

{% block content %}

    <h1>Videos</h1>
    <ul>
        {% for video in videos %}
            <li>
                <iframe width="420" height="315"
                        src="https://www.youtube.com/embed/{{ video.youtube_id }}">
                </iframe>
            </li>
        {% endfor %}
    </ul>


{% endblock %}