{% extends 'layout.html.twig' %}
{% block figthers %}
    <div class="container-fluid">

        <div class="row">
            <h6 class="nav-link col-12">Who is the best DJ of the month?</h6>

            <ul class="pagination">
                {% set i = 0 %}
                {% for userData in users %}

                    <li class="page-item">

                        <div class="card card-fighters">
                            <img {% if userData.github %}src="https://avatars.githubusercontent.com/{{ userData.github }}" {% else %} src="../../../public/assets/images/dj.jpeg" {% endif %}
                                     class="card-img-top" alt="image of a DJ">
                            <div class="card-body fighters-name">{{ userData.pseudo }}</div>
                            <p class="netflix-number card-text">{% set i = i + 1 %}{{ i }}</p>
                        </div>

                    </li>

                {% endfor %}
            </ul>
        </div>
    </div>
{% endblock %}


